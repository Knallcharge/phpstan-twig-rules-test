{
  "type": "project",
  "license": "proprietary",
  "prefer-stable": true,
  "require": {
    "php": ">=7.2.5",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "symfony/console": "5.3.*",
    "symfony/dotenv": "5.3.*",
    "symfony/flex": "^1.3.1",
    "symfony/framework-bundle": "5.3.*",
    "symfony/runtime": "5.3.*",
    "symfony/twig-bundle": "5.3.*",
    "symfony/yaml": "5.3.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.1",
    "phpstan/extension-installer": "^1.1",
    "symplify/phpstan-extensions": "^10.0@beta",
    "symplify/phpstan-rules": "^10.0@beta",
    "symplify/phpstan-twig-rules": "^10.0.x-dev"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  }
}
